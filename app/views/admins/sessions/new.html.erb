<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <h1>Conceito Admin</h1>
  <div>
     <%= f.email_field :email, class: "form-control", autofocus: true, placeholder: 'Email'  %>
  </div>
  <div>
    <%= f.password_field :password, autocomplete: "off", class: "form-control", placeholder: 'Senha' %>
  </div>
  <% if devise_mapping.rememberable? -%>
  <!--
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>-->
  
  <div>
    <%= f.submit "Login", class: 'btn btn-success submit"' %>
    <%= render "admins/shared/links" %>
  </div>

  <div class="clearfix"></div>
  <div class="separator">
    <div class="clearfix"></div>
    <br />
  </div>
<% end %>


